  "name": "sfdc-tasks",
  "version": "1.0.1",
  "description": "React app for displaying tasks in SFDC",
  "main": "src/index.jsx",
  "scripts": {
    "start": "webpack-dev-server --env.env development",
    "build": "webpack --env.env production",
    "build:salesforce": "webpack --env.env production --env.salesforce true",
    "storybook": "start-storybook -p 9001 -c .storybook --dont-track",
    "stats": "webpack --env.env stats",
    "stats:json": "webpack --env.env production --json > stats.json",
    "serve:prod": "npm run build && lite-server",
    "test": "karma start --single-run true",
    "test:watch": "karma start --single-run false",
    "predeploy": "npm run build:salesforce",
    "deploy": "gulp deploy"
  },
